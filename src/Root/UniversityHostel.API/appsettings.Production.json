{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=UniversityHostelDb;Trusted_Connection=True;"
  },
  "StateLog": {
    "PerformancePath": "Logs/Performance/{year}/{month}/{day}-performance.json",
    "UserPath": "Logs/User/{year}/{month}/{day}-user.json",
    "ErrorPath": "Logs/Errors/{year}/{month}/{day}/{userid}.json"
  },
  "JwtConfiguration": {
    "Issuer": "UniversityHostel.tk",
    "Audience": "https://localhost:443",
    "DurationInMinutes": 10
  },
  "AppSettings": {
    "Migrate": true
  }
}
